<h1>New Song Form</h1>
<form action="/songs" method="post">

  <label>Name:</label>
  <input id="Name" type="text" name="song[name]">

  <br>

  <label>Choose existing songs:</label>
  <%Song.all.each do |song|%>
  <input id="<%=song.id%> "type="checkbox" name="song[name]" value="name"><%=song%></input>
  <%end%>

  <input type="submit" value="submit">
</form>
